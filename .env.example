# AlphaStock Trading System - Environment Configuration Template
# Copy this file to .env.dev and fill in your actual credentials

# KITE CONNECT API CREDENTIALS (Required)
KITE_API_KEY=your_api_key_here
KITE_API_SECRET=your_api_secret_here
KITE_ACCESS_TOKEN=your_access_token_here
KITE_REQUEST_TOKEN=
KITE_USER_ID=

# TRADING CONFIGURATION
PAPER_TRADING=true
PAPER_CAPITAL=100000

# SYSTEM CONFIGURATION
LOG_LEVEL=INFO
DEBUG_MODE=false

# DATABASE CONFIGURATION (Optional)
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=8123
CLICKHOUSE_DATABASE=alphastock
CLICKHOUSE_USERNAME=default
CLICKHOUSE_PASSWORD=

POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DATABASE=alphastock
POSTGRES_USERNAME=postgres
POSTGRES_PASSWORD=

REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
